    <!-- About Intro-->
    <article class="intro" id="intro">
      <h2 class="title"><span><%= t('about.about') %></span></h2>
      <aside>
        <div class="logo">
          <img src="images/v3/about/shield-logo.png" width="176" height="231" alt="Crowdvoice - Tracking Voices of Protest">
        </div>
        <div class="contact-info-wrapper">
          <div class="contact">
            <h3 class="subtitle"><%= t('about.contact') %></h3>
            <p><a href="mailto:crowdvoice@mideastyouth.com">crowdvoice@mideastyouth.com</a></p>
          </div>
          <div class="connect">
            <h3 class="subtitle"><%= t('about.connect') %></h3>
            <ul>
              <li class="js-tw-follow"></li>
              <li class="js-tw-tweet"></li>
              <li class="js-fb-like"></li>
            </ul>
          </div>
        </div>
      </aside>
      <div class="content">
        <p class="about-message"><%= t('about.about_message_first') %></p>
        <p><%= t('about.about_message_second') %></p>
        <p><%= t('about.about_message_third') %></p>
        <h3><%= t('about.founding') %></h3>
        <p class="static-sec-text"><%= t('about.founding_message') %></p>
        <a href="http://www.shuttleworthfoundation.org/" target="_blank"><img class="shuttleworth-logo" src="/images/shuttleworth_logo.png"></a>
      </div>
    </article><!-- @intro -->

    <!-- What we do -->
    <article class="what-we-do" id="what-we-do">
      <h2 class="title"><span><%= t('about.what_we_do.title') %></span></h2>
      <div class="content">
        <div>
          <div class="sprite amplify"></div>
          <h3><%= t('about.what_we_do.amplify') %></h3>
          <p><%= t('about.what_we_do.amplify_message') %></p>
        </div>
        <div>
          <div class="sprite curate"></div>
          <h3><%= t('about.what_we_do.curate') %></h3>
          <p><%= t('about.what_we_do.curate_message') %></p>
        </div>
        <div>
          <div class="sprite educate"></div>
          <h3><%= t('about.what_we_do.educate') %></h3>
          <p><%= t('about.what_we_do.educate_message') %></p>
        </div>
        <div>
          <div class="sprite engage"></div>
          <h3><%= t('about.what_we_do.engage') %></h3>
          <p><%= t('about.what_we_do.engage_message') %></p>
        </div>
      </div>
    </article><!-- @what-we-do -->

    <!-- HOW IT WORKS -->
