          <div class="widget-tab-pane visible-on-medium-view" id="getting-started">
            <!-- getting started tabs -->
            <div class="widget-tabs tabs-vertical">
              <ul class="widget-tabs-nav hiw-secondary-tabs hidden-on-medium-view">
                <% (1..4).each do |i| %>
                  <li>
                    <a href="#" data-target="#gs<%=i%>" class="tab">
                      <span class="number"><%=i%></span>
                      <span class="text"><%= t("about.how_it_works.getting_started.item_0#{i}").html_safe %></span>
                    </a>
                  </li>
                <% end %>
              </ul>
              <div class="widget-tabs-content hiw-secondary-tabs-content">
                <p class="hiw-section-title visible-on-medium-view"><%= t('about.how_it_works.getting_started.title') %></p>
                <% (1..4).each do |i| %>
                  <div class="widget-tab-pane visible-on-medium-view" id="gs<%=i%>">
                    <div class="hiw-secondary-tabs-mobile visible-on-medium-view">
                      <span class="number"><%=i%></span>
                      <span class="text"><%= t("about.how_it_works.getting_started.item_0#{i}").html_safe %></span>
                    </div>
                    <img src="images/v3/about/how-it-works/01-0<%=i%>.jpg" width="322" height="322" alt='Crowdvoice - <%= t("about.how_it_works.getting_started.item_0#{i}") %> image'>
                  </div>
                <% end %>
              </div>
            </div>
            <!-- @getting-started -->
          </div>
          <div class="widget-tab-pane visible-on-medium-view" id="add-your-voice">
