<% end %>

<header class="about-page subnav fixed-fake">
  <div class="theme-image black"></div>
  <nav class="inner-subnav">
    <div class="content-700">
      <ul class="menu">
        <li><a href="#" data-target="intro" class="active">About &amp; Contact</a></li>
        <li><a href="#" data-target="what-we-do">What we do</a></li>
        <li><a href="#" data-target="how-it-works">How it works</a></li>
