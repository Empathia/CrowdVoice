            <div class="widget-tabs tabs-vertical">
              <ul class="widget-tabs-nav hiw-secondary-tabs hidden-on-medium-view">
                <% (1..6).each do |i| %>
                  <li>
                    <a href="#" data-target="#av<%=i%>" class="tab">
                      <span class="number"><%=i%></span>
                      <span class="text"><%= t("about.how_it_works.add_your_voice.item_0#{i}").html_safe %></span>
                    </a>
                  </li>
                <% end %>
              </ul>
