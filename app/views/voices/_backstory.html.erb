<% head do %>
  <% @is_gaza = @voice.is_witness_gaza %>
  <%= include_stylesheets :backstory %>
<% end %>

<%# cache("#{current_connection}_backstory_#{@voice.id}") do %>
<header class="main-header">
  <%= render :partial => "nav_bar" %>
</header>

<!--<div class="sweeper header-sep"></div>-->

<div class="cv-backstory-ui-wrapper" data-background="<%= @voice.background %>">
  <div class="backstory-map-wrapper" 
      data-lat="<%= @voice.latitude.present? ? @voice.latitude : '-34.397' %>"
      data-lng="<%= @voice.longitude.present? ? @voice.longitude : '150.644' %>">
  </div>
</div>

<%= include_javascripts :backstory_pack %>
<script type="text/javascript">
  new FacebookNavButton({
    fbPath : "<%= fb_path %>"
  });
</script>


